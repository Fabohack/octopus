        <%= @organizacion.nombre_organizacion %>
      </h1>
    </section>
  </aside>
  <div class="col-md-8">
    <% if @organizacion.objetivos.any? %>
      <h3>Objetivos Estrat√©gicos (<%= @organizacion.objetivos.count %>)</h3>
